<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#5E98F1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <FrameLayout
            android:id="@+id/frameLayout4"
            android:layout_width="413dp"
            android:layout_height="179dp"
            android:background="#BCD6FF"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="157dp">

        </FrameLayout>

        <TextView
            android:id="@+id/textView7"
            android:layout_width="300dp"
            android:layout_height="36dp"
            android:text="Contractor editor"
            android:textAlignment="center"
            android:textColor="#000000"
            android:textSize="34sp"
            tools:layout_editor_absoluteX="56dp"
            tools:layout_editor_absoluteY="47dp" />

        <Button
            android:id="@+id/button_edit"
            android:layout_width="411dp"
            android:layout_height="58dp"
            android:backgroundTint="#3B70F1"
            android:text="Apply"
            android:textSize="24sp"
            tools:layout_editor_absoluteY="563dp" />

        <Button
            android:id="@+id/button_backButtonEdit"
            android:layout_width="411dp"
            android:layout_height="58dp"
            android:backgroundTint="#3B70F1"
            android:text="Back"
            android:textSize="24sp"
            tools:layout_editor_absoluteY="650dp" />

        <TextView
            android:id="@+id/textView15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Name:"
            android:textSize="24sp"
            tools:layout_editor_absoluteX="24dp"
            tools:layout_editor_absoluteY="183dp" />

        <TextView
            android:id="@+id/textView16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Surname:"
            android:textSize="24sp"
            tools:layout_editor_absoluteX="24dp"
            tools:layout_editor_absoluteY="254dp" />

        <EditText
            android:id="@+id/editTextText_conNameEditor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="text"
            android:text="Name"
            tools:layout_editor_absoluteX="191dp"
            tools:layout_editor_absoluteY="179dp" />

        <EditText
            android:id="@+id/editTextText_conSurnameEditor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="text"
            android:text="Name"
            tools:layout_editor_absoluteX="189dp"
            tools:layout_editor_absoluteY="241dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <Button
        android:id="@+id/button_edit4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="563dp"
        android:layout_marginBottom="29dp"
        android:backgroundTint="#3B70F1"
        android:text="Apply"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/button_backButtonEdit4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_backButtonEdit4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="23dp"
        android:backgroundTint="#3B70F1"
        android:text="Back"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_edit4" />

</androidx.constraintlayout.widget.ConstraintLayout>